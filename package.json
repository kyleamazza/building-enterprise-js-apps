{
  "name": "hobnob",
  "version": "0.1.0",
  "description": "Building Enterprise JavaScript Applications, Daniel Li",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel src -d dist",
    "serve": "yarn run build && node dist/index.js",
    "watch": "nodemon -w src --exec yarn run serve",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "test:e2e": "npx cucumber-js spec/cucumber/features/ --require-module @babel/register --require spec/cucumber/steps/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyleamazza/building-enterprise-js-apps.git"
  },
  "author": "Kyle Mazza <kyleamazza@gmail.com> (http://www.kyleamazza.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyleamazza/building-enterprise-js-apps/issues"
  },
  "homepage": "https://github.com/kyleamazza/building-enterprise-js-apps#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "cucumber": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^1.3.1",
    "nodemon": "^1.18.11",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3"
  }
}
